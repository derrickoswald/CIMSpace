    for svg in $@; do

        icon=$(basename $svg .svg)
        echo ${icon}

        inkscape \
            --export-dpi=96 \
            --export-type=png \
            --export-file=png/${icon}.png \
            $svg &> /dev/null

        inkscape \
            --export-dpi=192 \
            --export-type=png \
            --export-file=png/${icon}@2x.png \
            $svg &> /dev/null
    done

